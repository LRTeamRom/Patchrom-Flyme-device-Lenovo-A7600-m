<?xml version="1.0" encoding="utf-8"?>
<preference-headers
  xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <header android:id="@id/wireless_section" android:title="@string/header_category_wireless_networks" />
    <header android:icon="@drawable/ic_settings_airplane" android:id="@id/toggle_airplane" android:title="@string/airplane_mode" />
    <header android:icon="@drawable/ic_settings_wireless" android:id="@id/wifi_settings" android:title="@string/wifi_settings">
        <intent android:targetPackage="com.android.wifi" android:action="android.intent.action.MAIN" android:targetClass="com.android.wifi.Settings$WifiSettingsActivity">
            <extra android:name="home_as_up_enable" android:value="true" />
        </intent>
    </header>
    <header android:icon="@drawable/ic_settings_dataconnect" android:id="@id/header_id_plugin" android:title="@string/mobile_network_settings_title" android:dependency="toggle_airplane" android:breadCrumbTitle="mobile_network_settings">
        <intent android:targetPackage="com.lenovo.simsettings" android:action="android.intent.action.MAIN" android:targetClass="com.lenovo.simsettings.MobileNetworkSettings" />
    </header>
    <header android:icon="@drawable/ic_settings_dataconnect" android:id="@id/header_id_plugin" android:title="@string/mobile_network_settings_title" android:dependency="toggle_airplane" android:breadCrumbTitle="mobile_network_settings">
        <intent android:targetPackage="com.lenovo.leos.mobileNetworkSettings" android:action="android.settings.DATA_ROAMING_SETTINGS" android:targetClass="com.lenovo.leos.mobileNetworkSettings.MobileNetworkSettings" />
    </header>
    <header android:icon="@drawable/ic_personal_hotspot" android:id="@id/header_id_plugin" android:title="@string/personal_hotsport_title" android:breadCrumbTitle="personal_hotspot">
        <intent android:targetPackage="com.lenovo.android.settings.tether" android:action="android.intent.action.MAIN" android:targetClass="com.lenovo.android.settings.tether.TetherSettings" />
    </header>
    <header android:icon="@drawable/ic_settings_bluetooth2" android:id="@id/bluetooth_settings" android:title="@string/bluetooth_settings_title" android:fragment="com.android.settings.bluetooth.BluetoothSettings" />
    <header android:id="@id/operator_settings" android:fragment="com.android.settings.WirelessSettings">
        <intent android:action="com.android.settings.OPERATOR_APPLICATION_SETTING" />
    </header>
    <header android:icon="@drawable/ic_settings_more" android:id="@id/wireless_settings" android:title="@string/radio_controls_title" android:fragment="com.lenovo.settings.LenovoWirelessSettings" android:breadCrumbTitle="@string/wireless_networks_settings_title" />
    <header android:id="@id/personal_section" android:title="@string/header_category_personal" />
    <header android:icon="@drawable/ic_settings_volumn" android:id="@id/header_id_plugin" android:title="@string/ringtone_settings" android:breadCrumbTitle="ringtone_settings">
        <intent android:targetPackage="com.android.profilesettings" android:action="android.intent.action.MAIN" android:targetClass="com.android.profilesettings.profile.SoundSettings" />
    </header>
    <header android:icon="@drawable/ic_settings_display" android:id="@id/display_settings" android:title="@string/display_settings" android:fragment="com.android.settings.DisplaySettings" />
    <header android:icon="@drawable/ic_settings_lockscreen" android:id="@id/header_id_plugin" android:title="@string/locks_settings_title" android:breadCrumbTitle="lock_settings">
        <intent android:targetPackage="com.lenovo.keyguard.settings" android:action="android.intent.action.MAIN" android:targetClass="com.lenovo.keyguard.settings.KeyguardSettingsActivity" />
    </header>
    <header android:icon="@drawable/ic_settings_notification" android:id="@id/header_id_plugin" android:title="@string/notification_settings_title" android:breadCrumbTitle="notification_settings">
        <intent android:targetPackage="com.android.systemui" android:action="android.intent.action.MAIN" android:targetClass="com.android.systemui.lenovo.settings.NotificationSettings" />
    </header>
    <header android:icon="@drawable/ic_char_settings" android:id="@id/character_settings" android:title="@string/character_settings" android:fragment="com.lenovo.settings.CharSettings" />
    <header android:id="@id/base_section" android:title="@string/base_settings_title" />
    <header android:icon="@drawable/ic_settings_date_time" android:id="@id/date_time_settings" android:title="@string/date_and_time_settings_title" android:fragment="com.android.settings.DateTimeSettings" />
    <header android:icon="@drawable/ic_settings_language" android:id="@id/language_settings" android:title="@string/language_settings" android:fragment="com.android.settings.inputmethod.InputMethodAndLanguageSettings" />
    <header android:icon="@drawable/ic_settings_call" android:id="@id/header_id_plugin" android:title="@string/call_settings_title" android:dependency="toggle_airplane" android:breadCrumbTitle="call_settings">
        <intent android:targetPackage="com.lenovo.callsetting" android:targetClass="com.mediatek.settings.CallSettings" />
    </header>
    <header android:icon="@drawable/ic_settings_schpwronoff" android:id="@id/header_id_plugin" android:title="@string/schedule_power_on_off_settings_title" android:breadCrumbTitle="power_setting">
        <intent android:action="com.android.settings.SCHEDULE_POWER_ON_OFF_SETTING" />
    </header>
    <header android:icon="@drawable/ic_settings_multisim" android:id="@id/header_id_plugin" android:title="@string/gemini_sim_management_title" android:dependency="toggle_airplane" android:breadCrumbTitle="multi_sim_settings">
        <intent android:targetPackage="com.lenovo.geminisettings" android:action="android.settings.GEMINI_MANAGEMENT" android:targetClass="com.lenovo.geminisettings.Settings" />
    </header>
    <header android:icon="@drawable/ic_settings_multisim" android:id="@id/header_id_plugin" android:title="@string/gemini_sim_management_title" android:dependency="toggle_airplane" android:breadCrumbTitle="multi_sim_settings">
        <intent android:targetPackage="com.lenovo.leos.simsettings" android:action="android.intent.action.SimSettings" android:targetClass="com.lenovo.leos.simsettings.SimSettingsActivity" />
    </header>
    <header android:icon="@drawable/ic_settings_location" android:id="@id/location_settings" android:title="@string/location_settings_title" android:fragment="com.android.settings.location.LocationSettings" />
    <header android:id="@id/account_settings" android:title="@string/header_category_account_settings" />
    <header android:icon="@drawable/ic_lenovo_cloud" android:id="@id/lenovo_cloud">
        <intent android:action="com.lenovo.leos.cloud.sync.intent.action.SYNC_SETTING" />
    </header>
    <header android:icon="@drawable/ic_settings_accounts" android:id="@id/account_add" android:title="@string/account_settings" android:fragment="com.android.settings.accounts.AccountSettings" />
    <header android:id="@id/system_section" android:title="@string/system_settings_title" />
    <header android:icon="@drawable/ic_settings_security" android:id="@id/security_settings" android:title="@string/security_settings_title" android:fragment="com.android.settings.SecuritySettings" />
    <header android:icon="@drawable/ic_settings_fingerprint" android:id="@id/header_id_plugin" android:title="@string/fingerprint_settings_title" android:breadCrumbTitle="fingerprint_settings">
        <intent android:targetPackage="com.lenovo.lenovofingerprintsettings" android:action="android.intent.action.MAIN" android:targetClass="com.lenovo.lenovofingerprintsettings.FingerprintManagerActivity" />
    </header>
    <header android:icon="@drawable/ic_settings_lepower" android:id="@id/header_id_plugin" android:title="@string/lepower_settings_title" android:breadCrumbTitle="lepower_settings">
        <intent android:action="com.lenovo.powersetting.intent.action.ACTION_START_MAIN" />
    </header>
    <header android:icon="@drawable/ic_settings_storage" android:id="@id/storage_settings" android:title="@string/storage_settings" android:fragment="com.android.settings.deviceinfo.Memory" />
    <header android:icon="@drawable/ic_settings_applications" android:id="@id/header_id_plugin" android:title="@string/applications_settings_title" android:breadCrumbTitle="applications_settings">
        <intent android:targetPackage="com.lenovo.security" android:action="android.intent.action.MAIN" android:targetClass="com.android.settingsapplication.ApplicationActivity" />
    </header>
    <header android:icon="@drawable/ic_settings_backup" android:id="@id/privacy_settings" android:title="@string/privacy_settings" android:fragment="com.android.settings.PrivacySettings" />
    <header android:icon="@drawable/ic_settings_accessibility" android:id="@id/accessibility_settings" android:title="@string/accessibility_settings" android:fragment="com.android.settings.accessibility.AccessibilitySettings" />
    <header android:icon="@drawable/ic_settings_print" android:id="@id/print_settings" android:title="@string/print_settings" android:fragment="com.android.settings.print.PrintSettingsFragment" />
    <header android:icon="@drawable/ic_settings_development" android:id="@id/development_settings" android:title="@string/development_settings_title" android:fragment="com.android.settings.DevelopmentSettings" />
    <header android:icon="@drawable/ic_settings_about" android:id="@id/about_settings" android:title="@string/about_settings" android:fragment="com.lenovo.settings.AboutActivity" />
</preference-headers>